    }
   ],
   "source": [
    "model.compile(optimizer='sgd', loss = 'binary_crossentropy', metrics=['accuracy'])\n",
    "\n",
    "history = model.fit(x_train_onehot, y_train, epochs=20, batch_size = 32, validation_data=(x_val_onehot, y_val))"
   ]
